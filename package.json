{
  "name": "federalist-uswds-jekyll",
  "version": "1.3.0",
  "scripts": {
    "build": "bundle exec jekyll build",
    "clean": "bundle exec jekyll clean",
    "reset": "npx rimraf .git",
    "start": "bundle exec jekyll serve",
    "federalist": "npm link @angular/cli; npm run build:${BRANCH}",
    "start-detached": "bundle exec jekyll serve --detach",
    "htmlproofer": "bundle exec htmlproofer _site",
    "pa11y-ci:sitemap": "pa11y-ci --sitemap http://localhost:4000/sitemap.xml --sitemap-exclude \"/*.pdf\"  --sitemap-exclude \"/admin\"",
    "ng": "ng",
    "start:ang": "ng serve --configuration development",
    "build:ang": "ng build --configuration dev",
    "build:dev": "ng build --configuration dev --base-href=https://federalist-7a132a2e-6307-4cd0-9f82-e30e871d214a.sites.pages.cloud.gov/preview/gsa/section508.gov/development-art/art/",
    "build:ECASOGP-March": "ng build --configuration dev --base-href=https://federalist-0d16eea5-7789-4e2b-93f0-5cffcda12006.sites.pages.cloud.gov/preview/gsa/gtm/ECASOGP-March/art/",
    "build:test": "ng build --configuration test --base-href=https://federalist-7a132a2e-6307-4cd0-9f82-e30e871d214a.sites.pages.cloud.gov/preview/gsa/section508.gov/test-art/art/",
    "build:staging": "ng build --configuration staging --base-href=https://federalist-7a132a2e-6307-4cd0-9f82-e30e871d214a.sites.pages.cloud.gov/preview/gsa/section508.gov/staging-art/art/",
    "build:prod": "ng build --configuration production --base-href=https://www.section508.gov/art/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "dependencies": {
    "netlify-cms": "^2.10.102",
    "uswds": "~2.12.1",
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@fortawesome/fontawesome-free": "~6.2.0",
    "@uswds/uswds": "~3.2.0",
    "docx": "^8.0.2",
    "ngx-clipboard": "^15.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "pa11y-ci": "^2.4.0",
    "rimraf": "^3.0.2",
    "@angular-devkit/build-angular": "^14.2.6",
    "@angular/cli": "~14.2.6",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.7.2"
  }
}
